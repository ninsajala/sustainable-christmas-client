rt { Link, withRouter } from 'react-router-dom';\nimport AddFavorite from './DetailsComponents/AddFavorite';\nimport RemoveFavorite from './DetailsComponents/RemoveFavorite';\nimport CommentSection from './DetailsComponents/CommentSection';\n\nfunction TipDetails(props) {\n  const [tipDetails, setTipDetails] = useState({});\n  const [loaded, setLoaded] = useState(false);\n  const { params } = props.match;\n\n  const getTipDetails = () => {\n    axios\n      .get(\n        `https://sustainable-christmas-server.herokuapp.com/tips/${params.id}`\n      )\n      .then((foundTip) => {\n        setTipDetails(foundTip.data);\n        setLoaded(true);\n      });\n  };\n\n  useEffect(() => {\n    getTipDetails();\n  }, []);\n\n  const handleDeleteTip = () => {\n    axios\n      .delete(\n        `https://sustainable-christmas-server.herokuapp.com/tips/${tipDetails._id}`\n      )\n      .then(() => props.history.push(`/tips`));\n  };\n\n  const checkIfOwner = () => {\n    if (tipDetails.author._id === props.loggedInUser._id) {\n      return (\n        <div className='form-group button-group'>\n          <Link to={`/tips/edit/${tipDetails._id}`}>\n            <button className='btn btn-warning btn-sm'>\n              <i className='fas fa-edit'></i>\n            </button>\n          </Link>\n\n          <button className='btn btn-danger btn-sm' onClick={handleDeleteTip}>\n            <i className='fas fa-trash-alt'></i>\n          </button>\n        </div>\n      );\n    }\n  };\n\n  const checkIfFavorite = () => {\n    if (\n      props.loggedInUser.favorites.find(\n        (element) => element._id === tipDetails._id\n      )\n    ) {\n      return (\n        <RemoveFavorite\n          loggedInUser={props.loggedInUser}\n          tipDetails={tipDetails}\n          updateTip={getTipDetails}\n          checkFavorite={checkIfFavorite}\n          getUser={props.getUser}\n        />\n      );\n    } else {\n      return (\n        <AddFavorite\n          loggedInUser={props.loggedInUser}\n          tipDetails={tipDetails}\n          updateTip={getTipDetails}\n          checkFavorite={checkIfFavorite}\n          getUser={props.getUser}\n        />\n      );\n    }\n  };\n\n  return props.loggedInUser ? (\n    <div>\n      {loaded ? (\n        <div className='tipDetails'>\n          <h3>{tipDetails.title}</h3>\n          <div className='tipDetailHeaderInfo'>\n            <div className='authorSection'>\n              {checkIfOwner()}\n              <p>\n                Written by {}\n                <Link to={`/profile/${tipDetails.author._id}`}>\n                  <b>\n                    {tipDetails.author.firstName} {tipDetails.author.lastName}\n                  </b>\n                </Link>\n              </p>\n            </div>\n            <div className='favoriteSection'>\n              <p>\n                {tipDetails.addedToFavorites.length} times added to favorites\n              </p>\n              {checkIfFavorite()}\n            </div>\n          </div>\n          <div className='tipDetailsMain'>\n            {tipDetails.picture.length <= 0 ? (\n              <div className='detailsPicture altDetails'></div>\n            ) : (\n              <div\n                className='detailsPicture'\n                style={{\n                  backgroundImage: `url(${tipDetails.picture})`,\n                }}></div>\n            )}\n            <p>{tipDetails.content}</p>\n            {tipDetails.extraInfo && (\n              <a href={tipDetails.extraInfo} rel='noreferrer' target='_blank'>\n                <button className='btn btn-warning'>More info</button>\n              </a>\n            )}\n          </div>\n          <div className='tipDetailsBottomSection'>\n            <CommentSection\n              getTipDetails={getTipDetails}\n              tipDetails={tipDetails}\n              loggedInUser={props.loggedInUser}\n            />\n          </div>\n        </div>\n      ) : (\n        <p className='logInWarning'>Loading details..</p>\n      )}\n    </div>\n  ) : (\n    <p className='logInWarning'>\n      Please log in to see this page <br />\n      <Link to={'/login'}>\n        <button className='btn btn-warning'>Login</button>\n      </Link>\n    </p>\n  );\n}\n\nexport default withRouter(TipDetails);\n","/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/TipSearch.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/reportWebVitals.js",[],["136","137"],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/services/upload-service.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/services/auth-service.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Layout/PlayMusic.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Profile/EditProfile.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Profile/TipsListItem.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Profile/FavoriteListItem.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Recipes/RecipeDetails.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Recipes/RecipeInList.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Recipes/RecipeSearch.js",[],["138","139"],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/charity/CharityList.js",[],"/Users/nina/Documents/Nina/Ironhack/projec