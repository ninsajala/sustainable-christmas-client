ops.loggedInUser._id) {\n      return (\n        <div>\n          <Link to={`/tips/edit/${tipDetails._id}`}>Edit Tip</Link>\n          <button onClick={handleDelete}>Remove Tip</button>\n        </div>\n      );\n    }\n  };\n\n  return props.loggedInUser ? (\n    <div>\n      {loaded ? (\n        <div>\n          <h3>{tipDetails.title}</h3>\n          <p>By {tipDetails.author.firstName}</p>\n          <p>{tipDetails.content}</p>\n          <img src={tipDetails.picture} alt={tipDetails.title} />\n          {!props.loggedInUser.favorites.includes(tipDetails._id) && (\n            <button onClick={addToFavorites}>Add to Favorites</button>\n          )}\n          {props.loggedInUser.favorites.includes(tipDetails._id) && (\n            <p>Is added to favorites</p>\n          )}\n          {checkIfOwner()}\n          {tipDetails.comments &&\n            tipDetails.comments.map((item) => (\n              <div className='oneComment'>\n                <q key={item._id}>{item.content}</q>\n              </div>\n            ))}\n          <Comment\n            tip={tipDetails._id}\n            user={props.loggedInUser._id}\n            updateTip={getTipDetails}\n          />\n        </div>\n      ) : (\n        <div>Loading tip</div>\n      )}\n    </div>\n  ) : (\n    <Redirect to='/login' />\n  );\n}\n\nexport default withRouter(TipDetails);\n","/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/Comment.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Homepage/Home.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/EditTip.js",["93"],{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"94","replacedBy":"98"},{"ruleId":"96","replacedBy":"99"},{"ruleId":"94","replacedBy":"100"},{"ruleId":"96","replacedBy":"101"},{"ruleId":"94"