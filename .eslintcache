       }\n      )\n      .then((response) => {\n        props.getUser(response.data.updatedUser1);\n        setUserData(response.data.updatedUser2);\n      });\n  }\n\n  function unFollowUser() {\n    myID = props.loggedInUser._id;\n    otherUserId = userData._id;\n\n    axios\n      .put(\n        `https://sustainable-christmas-server.herokuapp.com/unfollow`,\n        {\n          myID,\n          otherUserId,\n        },\n        {\n          withCredentials: true,\n        }\n      )\n      .then((response) => {\n        props.getUser(response.data.updatedUser1);\n        setUserData(response.data.updatedUser2);\n      });\n  }\n\n  function checkFollowing() {\n    if (userData.followers.includes(props.loggedInUser._id)) {\n      return (\n        <button className='btn btn-warning' onClick={unFollowUser}>\n          Unfollow\n        </button>\n      );\n    } else {\n      return (\n        <button className='btn btn-warning' onClick={followUser}>\n          Follow\n        </button>\n      );\n    }\n  }\n\n  useEffect(() => {\n    getUserData();\n  }, []);\n\n  return userData ? (\n    <div className='profileWrapper'>\n      {loaded ? (\n        <div className='userProfile'>\n          <h4>{userData.firstName}'s Profile</h4>\n          <div className='userInfo'>\n            <div className='pictureWrap'>\n              {userData.picture.length <= 0 ? (\n                <div className='profilePicture Alt'></div>\n              ) : (\n                <div\n                  className='profilePicture'\n                  style={{\n                    backgroundImage: `url(${userData.picture})`,\n                  }}></div>\n              )}\n            </div>\n            {userData.about && (\n              <div className='about'>\n                <h5>\n                  About {userData.firstName} {userData.lastName}:\n                </h5>\n                <p>{userData.about}</p>\n                <div className='followSection'>\n                  {checkFollowing()}{' '}\n                  <p>Followed by {userData.followers.length}</p>\n                </div>\n              </div>\n            )}\n          </div>\n          <h5>Tips written by {userData.firstName}</h5>\n          <div className='profileSection'>\n            {userData.tips.length <= 0 && (\n              <div>\n                <p>No tips added yet</p>\n              </div>\n            )}\n            {userData.tips &&\n              userData.tips.map((item) => (\n                <OtherProfileTipList item={item} key={item._id} />\n              ))}\n          </div>\n        </div>\n      ) : (\n        <p>Finding User</p>\n      )}\n    </div>\n  ) : (\n    <p className='logInWarning'>\n      Please log in to see this page <br />\n      <Link to={'/login'}>\n        <button className='btn btn-warning'>Login</button>\n      </Link>\n    </p>\n  );\n}\n\nexport default withRouter(OtherProfile);\n",{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","severity":1,"message":"148","line":28,"column":6,"nodeType":"149","endLine":28,"endColumn":8,"suggestions":"150"},{"ruleId":"147","severity":1,"message":"151","line":27,"column":6,"nodeType":"149","endLine":27,"endColumn":8,"suggestions":"152"},{"ruleId":"143","replacedBy":"153"},{"ruleId":"145","replacedBy":"154"},{"ruleId":"143","replacedBy":"155"},{"ruleId":"145","replacedBy":"156"},{"ruleId":"143","replacedBy":"157"},{"ruleId":"145","replacedBy":"158"},{"ruleId":"147","severity":1,"message":"159","line":85,"column":6,"nodeType":"149","endLine":85,"endColumn":8,"suggestions":"160"},"no-native-reassign",["161"],"no-negated-in-lhs",["162"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'params.id'. Either include it or remove the dependency array.","ArrayExpression",["163"],"React Hook useEffect has a missing dependency: 'getTipDetails'. Either include it or remove the dependency array.",["164"],["161"],["162"],["161"],["162"],["161"],["162"],"