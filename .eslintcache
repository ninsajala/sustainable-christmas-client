=> {\n    getRecipesFromApi('','');\n  }, []);\n\n  return (\n    <div className='recipeOverview'>\n      <div className='recipeIntro'>\n        <h2>Christmas Recipes</h2>\n        <p>\n          Cooking more environment friendly is easy, just keep these for things\n          in mind:\n        </p>\n        <ul>\n          <li>Local</li>\n          <li>Seasonal</li>\n          <li>Organic</li>\n          <li>Sustainable</li>\n        </ul>\n        <p>Browse the vegetarian recipes below for some inspiration!</p>\n        <RecipeSearch searchRecipes={getRecipesFromApi} />\n      </div>\n      <div>\n        {recipeData ? (\n          <div className='recipeList'>\n            {recipeData.map((recipe, index) => (\n              <RecipeInList key={index} recipe={recipe} />\n            ))}\n          </div>\n        ) : (\n          <div>Loading recipes</div>\n        )}\n      </div>\n    </div>\n  );\n}\n\nexport default Recipes;\n","/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Auth/Signup.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Layout/Nav.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Profile/MyProfile.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/Comment.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Layout/Footer.js",[],["116","117"],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/AddTip.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/EditTip.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/Auth/Login.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/TipDetails.js",["118","119"],"import React, { useState, useEffect } from 'react';\nimport axios from 'axios';\nimport Comment from './Comment';\nimport { Link, withRouter, Redirect } from 'react-router-dom';\nimport AddFavorite from './AddFavorite';\nimport RemoveFavorite from './RemoveFavorite';\n\nfunction TipDetails(props) {\n  const [tipDetails, setTipDetails] = useState({});\n  const [loaded, setLoaded] = useState(false);\n  const { params } = props.match;\n\n  const getTipDetails = () => {\n    axios\n      .get(\n        `http://localhost:5000/tips/${params.id}`\n        //`https://sustainable-christmas-server.herokuapp.com/tips/${params._id}`\n      )\n      .then((foundTip) => {\n        setTipDetails(foundTip.data);\n        setLoaded(true);\n      });\n  };\n\n  useEffect(() => {\n    getTipDetails();\n  }, []);\n\n  const handleDeleteTip = () => {\n    axios\n      .delete(\n        `http://localhost:5000/tips/${tipDetails._id}`\n        //`https://sustainable-christmas-server.herokuapp.com/tips/${tipDetails._id}`\n      )\n      .then(() => props.history.push(`/tips`));\n  };\n\n  const checkIfOwner = () => {\n    if (tipDetails.author._id === props.loggedInUser._id) {\n      return (\n        <div className='form-group button-group'>\n          <Link to={`/tips/edit/${tipDetails._id}`}>\n            <button className='btn btn-dark'>Edit Tip</button>\n          </Link>\n\n          <button className='btn btn-danger' onClick={handleDeleteTip}>\n            Remove Tip\n          </button>\n        </div>\n      );\n    }\n  };\n\n  const handleRemoveComment = (id) => {\n    axios\n      .delete(\n        `http://localhost:5000/comment/${id}`\n        //`https://sustainable-christmas-server.herokuapp.com/comment/${id}`\n      )\n      .then(() => {\n        getTipDetails();\n      });\n  };\n\n  const checkIfFavorite = () => {\n    if (\n      props.loggedInUser.favorites.find(\n        (element) => element._id === tipDetails._id\n      )\n    ) {\n      return (\n        <RemoveFavorite\n          loggedInUser={props.loggedInUser}\n          tipDetails={tipDetails}\n          updateTip={getTipDetails}\n          checkFavorite={checkIfFavorite}\n        />\n      );\n    } else {\n      return (\n        <AddFavorite\n          loggedInUser={props.loggedInUser}\n          tipDetails={tipDetails}\n          updateTip={getTipDetails}\n          checkFavorite={checkIfFavorite}\n        />\n      );\n    }\n  };\n\n  return props.loggedInUser ? (\n    <div>\n      {loaded ? (\n        <div className='tipDetails'>\n          <h3>{tipDetails.title}</h3>\n          <p>\n            Written by {tipDetails.author.firstName}{' '}\n            {tipDetails.author.lastName}\n          </p>\n          <p>Added to favorites by: {tipDetails.addedToFavorites.length}</p>\n          <img src={tipDetails.picture} alt={tipDetails.title} />\n          <p>{tipDetails.content}</p>\n          {tipDetails.extraInfo && (\n            <a href={tipDetails.extraInfo} rel='noreferrer' target='_blank'>\n              More info\n            </a>\n          )}\n          {checkIfFavorite()}\n          {checkIfOwner()}\n          {tipDetails.comments.length === 0 && <p>Leave the first comment</p>}\n          {tipDetails.comments.length > 0 && (\n            <div className='commentSection'>\n              <h5>Comments:</h5>\n              {tipDetails.comments.map((item) => (\n                <div className='oneComment'>\n                  <q key={item._id}>{item.content}</q>\n                  <p>- {item.author.firstName}</p>\n                  {props.loggedInUser.comments.includes(item._id) && (\n                    <button\n                      className='btn btn-dark'\n                      onClick={() => handleRemoveComment(item._id)}\n                      title={`Remove Comment ${item._id}`}>\n                      x\n                    </button>\n                  )}\n                </div>\n              ))}\n            </div>\n          )}\n          <Comment\n            tip={tipDetails._id}\n            user={props.loggedInUser._id}\n            updateTip={getTipDetails}\n          />\n        </div>\n      ) : (\n        <div>Loading tip</div>\n      )}\n    </div>\n  ) : (\n    <p>You need to be logged in to see this page</p>\n  );\n}\n\nexport default withRouter(TipDetails);\n","/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/RemoveFavorite.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/AddFavorite.js",["120"],"import React from 'react';\nimport axios from 'axios';\n\nfunction AddFavorite(props) {\n  let userId = props.loggedInUser._id;\n  let tipId = props.tipDetails._id;\n\n  function addToFavorites() {\n    axios\n      .put(\n        `http://localhost:5000/favorites/add`,\n        //`https://sustainable-christmas-server.herokuapp.com/favorites`,\n        { userId, tipId },\n        {\n          withCredentials: true,\n        }\n      )\n      .then(() => {\n        props.updateTip();\n        props.checkFavorite();\n        console.log('HALLO');\n      });\n  }\n\n  return (\n    <button\n      className='favoriteButton'\n      onClick={addToFavorites}\n      title='Add to Favorites'>\n      <i className='far fa-heart'></i>\n    </button>\n  );\n}\n\nexport default AddFavorite;\n","/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/components/ChristmasTips/TipSearch.js",[],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/reportWebVitals.js",[],["121","122"],"/Users/nina/Documents/Nina/Ironhack/project3-sustainable-christmas/sustainable-christmas-client/src/services/up